<h2><%= t('devise.sessions.new.sign_in') %></h2>

<%= form_with model: resource, url: session_path(resource_name), local: true do |f| %>
  <div>
    <%= f.label :email, t('devise.sessions.new.email') %><br />
    <%= f.email_field :email, autofocus: true, autocomplete: "email" %>
  </div>

  <div>
    <%= f.label :password, t('devise.sessions.new.password') %><br />
    <%= f.password_field :password, autocomplete: "current-password" %>
  </div>

  <div>
    <%= f.check_box :remember_me %>
    <%= f.label :remember_me, t('devise.sessions.new.remember_me') %>
  </div>

  <div>
    <%= f.submit t('devise.sessions.new.sign_in_button') %>
  </div>
<% end %>

<%= render "devise/shared/links" %>

